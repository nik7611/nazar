       \'d1\'f3\'f9\'e5\'f1\'f2\'e2\'f3\'fe\'f2 \'f0\'e0\'e7\'eb\'e8\'f7\'ed\'fb\'e5 \'f2\'e8\'ef\'fb \'eb\'e8\'f7\'ed\'ee\'f1\'f2\'e8. \'ce\'ed\'e8 \'ee\'f2\'eb\'e8\'f7\'e0\'fe\'f2\'f1\'ff \'ef\'ee \'f5\'e0\'f0\'e0\'ea\'f2\'e5\'f0\'f3, \'ea\'e0\'f7\'e5\'f1\'f2\'e2\'e0\'ec, \'f1\'ef\'ee\'f1\'ee\'e1\'ed\'ee\'f1\'f2\'ff\'ec \'e8 \'f1\'ea\'eb\'ee\'ed\'ed\'ee\'f1\'f2\'ff\'ec \'ea \'ee\'ef\'f0\'e5\'e4\'e5\'eb\'b8\'ed\'ed\'fb\'ec \'e2\'e8\'e4\'e0\'ec \'e4\'e5\'ff\'f2\'e5\'eb\'fc\'ed\'ee\'f1\'f2\'e8. \'ce\'ef\'e8\'f0\'e0\'ff\'f1\'fc \'ed\'e0 \'fd\'f2\'e8 \'ef\'ee\'eb\'ee\'e6\'e5\'ed\'e8\'ff, \'ec\'ee\'e6\'ed\'ee \'f1\'e4\'e5\'eb\'e0\'f2\'fc \'f1\'eb\'e5\'e4\'f3\'fe\'f9\'e8\'e5 \'e2\'fb\'e2\'ee\'e4\'fb: 1) \'e2 \'f1\'ee\'ee\'f2\'e2\'e5\'f2\'f1\'f2\'e2\'e8\'e8 \'f1 \'ef\'f1\'e8\'f5\'ee\'f4\'e8\'e7\'e8\'f7\'e5\'f1\'ea\'e8\'ec \'f2\'e8\'ef\'ee\'ec \'ea\'e0\'e6\'e4\'fb\'e9 \'e7\'e0\'e9\'ec\'e5\'f2 \'f1\'e2\'ee\'e5 \'ec\'e5\'f1\'f2\'ee \'e2 \'f1\'f2\'f0\'f3\'ea\'f2\'f3\'f0\'e5 \'ee\'e1\'f9\'e5\'f1\'f2\'e2\'e0; 2) \'e7\'e0\'ea\'ee\'ed\'ee\'ec\'e5\'f0\'ed\'fb\'ec \'f0\'e5\'e7\'f3\'eb\'fc\'f2\'e0\'f2\'ee\'ec \'e2\'fb\'ef\'ee\'eb\'ed\'e5\'ed\'e8\'ff \'fd\'f2\'ee\'e3\'ee \'f1\'f2\'e0\'ed\'e5\'f2 \'ed\'ee\'f0\'ec\'e0\'eb\'e8\'e7\'e0\'f6\'e8\'ff \'e2\'f1\'e5\'f5 \'f1\'f4\'e5\'f0 \'ed\'e0\'f8\'e5\'e9 \'e6\'e8\'e7\'ed\'e8. \'c8\'f1\'f2\'ee\'f0\'e8\'ff \lang1033\f1\'ab\lang1049\f0\'e4\'f0\'e5\'e2\'ed\'ee\'f1\'f2\'e8\lang1033\f1\'bb \lang1049\f0\'e8 \'f1\'f0\'e5\'e4\'ed\'e5\'e2\'e5\'ea\'ee\'e2\'fc\'ff \'e7\'ed\'e0\'e5\'f2 \'e2\'e0\'f0\'e8\'e0\'ed\'f2\'fb \'f1\'ee\'f6\'e8\'e0\'eb\'fc\'ed\'ee\'e9 \'ee\'f0\'e3\'e0\'ed\'e8\'e7\'e0\'f6\'e8\'e8, \'ea\'ee\'f2\'ee\'f0\'fb\'e5 \'ef\'f0\'ee\'f1\'f2\'fb \'e8 \'e5\'f1\'f2\'e5\'f1\'f2\'e2\'e5\'ed\'ed\'fb. \'d2\'e0\'ea, \'ed\'e0\'ef\'f0\'e8\'ec\'e5\'f0, \'e2 \lang1033\f1\'ab\lang1049\f0\'c4\'f0\'e5\'e2\'ed\'e5\'ec \'c8\'f0\'e0\'ed\'e5\lang1033\f1\'bb \lang1049\f0\'e2 \'f1\'ee\'ee\'f2\'e2\'e5\'f2\'f1\'f2\'e2\'e8\'e8 \'f1 \'ea\'e0\'f7\'e5\'f1\'f2\'e2\'e0\'ec\'e8 \'e8 \'f1\'ea\'eb\'ee\'ed\'ed\'ee\'f1\'f2\'ff\'ec\'e8 \'ea \'f2\'ee\'e9 \'e8\'eb\'e8 \'e8\'ed\'ee\'e9 \'e4\'e5\'ff\'f2\'e5\'eb\'fc\'ed\'ee\'f1\'f2\'e8 \'e2\'f1\'e5 \'eb\'fe\'e4\'e8 \'e4\'e5\'eb\'e8\'eb\'e8\'f1\'fc \'ed\'e0 \'f2\'f0\'e8 \lang1033\f1\'ab\lang1049\f0\'ef\'e8\'f8\'f2\'f0\'fb\lang1033\f1\'bb \emdash  \lang1049\f0\'e2\'ee\'e8\'ed\'fb (\'ee\'f0\'e3\'e0\'ed\'e8\'e7\'e0\'f2\'ee\'f0\'fb), \'f3\'f7\'e5\'ed\'fb\'e5 (\'f1\'e2\'ff\'f9\'e5\'ed\'ed\'ee\'f1\'eb\'f3\'e6\'e8\'f2\'e5\'eb\'e8), \'e7\'e5\'ec\'eb\'e5\'e4\'e5\'eb\'fc\'f6\'fb (\'ea\'ee\'ec\'ec\'e5\'f0\'f1\'e0\'ed\'f2\'fb). \'d2\'ee \'e6\'e5 (\'ed\'ee \'f1 \'e8\'ed\'fb\'ec\'e8 \'ed\'e0\'e7\'e2\'e0\'ed\'e8\'ff\'ec\'e8 \'f1\'ee\'f6\'e8\'e0\'eb\'fc\'ed\'fb\'f5 \'e3\'f0\'f3\'ef\'ef) \'ec\'fb \'ed\'e0\'f5\'ee\'e4\'e8\'ec \'e2 \lang1033\f1\'ab\lang1049\f0\'e4\'f0\'e5\'e2\'ed\'e5\'e9\lang1033\f1\'bb \lang1049\f0\'e8\'f1\'f2\'ee\'f0\'e8\'e8 \'c5\'e3\'e8\'ef\'f2\'e0, \'cc\'e5\'f1\'ee\'ef\'ee\'f2\'e0\'ec\'e8\'e8, \'c0\'ec\'e5\'f0\'e8\'ea\'e8, \'c8\'ed\'e4\'e8\'e8 \'e8 \'e2 \'e4\'f0\'f3\'e3\'e8\'f5 \'f0\'e0\'e7\'e2\'e8\'f2\'fb\'f5 \'f6\'e8\'e2\'e8\'eb\'e8\'e7\'e0\'f6\'e8\'ff\'f5.\par
